    "bcryptjs": "^2.4.3",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.23",
    "@types/node": "^22.5.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.5.3",
    "@types/bcryptjs": "^2.4.6"
  }
}
